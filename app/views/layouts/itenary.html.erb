<!--HTML Layout file for the Target controller -->


<!DOCTYPE html>

<html>  
  
    <head> 
        
    <title>MIRROR</title> 
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>
    <%= favicon_link_tag 'favicons/favicon.ico' %>
    <%= stylesheet_link_tag 'itenaries', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
    <link href="//maxcdn.bootstrapcdn.com/font-awesome/4.1.0/css/font-awesome.min.css" rel="stylesheet">
    </head>

  
    <body>
   
    <!-- Application heading partial -->
     
    <%= render "shared/application_heading" %>
    
    <!-- Navbar partial -->   
    
    <%= render "shared/nav", color: 'bg-light'%>
   



    <%= yield %>
  
    
    <br/>
    <br/>
    <br/>
    <br/>
    <br/>
<br/>
<br/>
<br/>
<br/>
    <!-- Footer partial --> 

    <%= render "shared/footer" %>

    </body>
</html>
<!-- End of the Story controller layout -->

