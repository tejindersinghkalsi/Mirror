<p id="notice"><%= notice %></p>

<h1>Feedbacks</h1>

<table>
  <thead>
    <tr>
      <th>Date</th>
      <th>Uipoints</th>
      <th>Usability</th>
      <th>Improvement</th>
      <th>Comments</th>
      <th>Addfeatures</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @feedbacks.each do |feedback| %>
      <tr>
        <td><%= feedback.date %></td>
        <td><%= feedback.uipoints %></td>
        <td><%= feedback.usability %></td>
        <td><%= feedback.improvement %></td>
        <td><%= feedback.comments %></td>
        <td><%= feedback.addfeatures %></td>
        <td><%= link_to 'Show', feedback %></td>
        <td><%= link_to 'Edit', edit_feedback_path(feedback) %></td>
        <td><%= link_to 'Destroy', feedback, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Feedback', new_feedback_path %>
